{
  "framework": "static",
  "buildCommand": "",
  "outputDirectory": ".",
  "install": "",
  "serverless": false,
  "staticFiles": {
    "pdfs": {
      "headers": {
        "Content-Type": "application/pdf",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    "*.pdf": {
      "headers": {
        "Content-Type": "application/pdf"
      }
    }
  },
  "routes": [
    {
      "src": "/pdfs/(.*\\.pdf)",
      "headers": {
        "Content-Type": "application/pdf"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
